{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/RFM95.elf",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "debugServerPath": "C:/ti/mspdebug/mspdebug.exe",
            "debugServerArgs": "tilib \"gdb\"",
            //"serverStarted": "MSP430Debug Server started",
            "miDebuggerPath": "C:/ti/msp430-gcc/bin/msp430-elf-gdb.exe",
            "miDebuggerServerAddress": "localhost:2000",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
